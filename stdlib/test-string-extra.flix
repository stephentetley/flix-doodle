
/////////////////////////////////////////////////////////////////////////////
// viewl                                                                   //
/////////////////////////////////////////////////////////////////////////////
@test
def viewl01() : Bool = 
    String2.viewl("") `assertEq!` None

@test
def viewl02() : Bool = 
    String2.viewl("hello") `assertEq!` Some('h', "ello")

/////////////////////////////////////////////////////////////////////////////
// viewr                                                                   //
/////////////////////////////////////////////////////////////////////////////
@test
def viewr01() : Bool = 
    String2.viewr("") `assertEq!` None

@test
def viewr02() : Bool = 
    String2.viewr("hello") `assertEq!` Some("hell", 'o')


/////////////////////////////////////////////////////////////////////////////
// commonSuffix                                                            //
/////////////////////////////////////////////////////////////////////////////
@test
def commonSuffix01(): Bool = 
    String2.commonSuffix("snail", "tail") `assertEq!` "ail"